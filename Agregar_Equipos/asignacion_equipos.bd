CREATE TABLE equipos (
    id INT AUTO_INCREMENT PRIMARY KEY,
    tipo_equipo VARCHAR(255) NOT NULL,
    nombre_host VARCHAR(255) NOT NULL,
    num_serie VARCHAR(255) NOT NULL,
    num_inventario VARCHAR(255) NOT NULL,
    encargado VARCHAR(255) NOT NULL,
    area_asignada VARCHAR(255) NOT NULL,
    ip VARCHAR(255),
    licencia_windows VARCHAR(255),
    licencia_office VARCHAR(255),
    antivirus VARCHAR(255),
    comentario TEXT
);
